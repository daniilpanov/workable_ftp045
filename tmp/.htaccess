AddHandler x-httpd-php56 .php

Options +FollowSymLinks
IndexIgnore */*
RewriteEngine On

RewriteCond %{REQUEST_URI} !^/(v3)
RewriteCond %{REQUEST_URI} !robots.txt
RewriteRule (.*) /v3/$1

RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_URI} !^/robots.txt$
RewriteRule . /v3/index.php